    <UserName>$jensthelandmand</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects>
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="True">
          <Destination Path="Data Source=landmandjens.database.windows.net,1433;Initial Catalog=jenslandbrug;User ID=Christoffer;Password=Chris123!" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="JensTheLandmand_v6.Models.ApplicationDbContext, JensTheLandmand_v6" MigrationConfiguration="JensTheLandmand_v6.Migrations.Configuration, JensTheLandmand_v6" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=landmandjens.database.windows.net,1433;Initial Catalog=jenslandbrug;User ID=Christoffer;Password=Chris123!</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
